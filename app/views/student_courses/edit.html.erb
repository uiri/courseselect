<h1>Editing student_course</h1>

<% form_for(@student_course) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :mark %><br />
    <%= f.text_field :mark %>
  </p>
  <p>
    <%= f.label :student_id %><br />
    <%= f.text_field :student_id %>
  </p>
  <p>
    <%= f.label :course_id %><br />
    <%= f.text_field :course_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @student_course %> |
<%= link_to 'Back', student_courses_path %>